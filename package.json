{
  "dependencies": {
    "express": "^4.19.2",
    "xget": "file:"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.10",
    "@cloudflare/workers-types": "^4.20250724.0",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.1.4",
    "wrangler": "^4.47.0"
  },
  "name": "xget",
  "private": false,
  "scripts": {
    "deploy": "wrangler pages deploy .",
    "dev": "wrangler dev",
    "format": "prettier --write src/ test/ *.js *.json",
    "format:check": "prettier --check src/ test/ *.js *.json",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "start": "wrangler pages dev .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit"
  },
  "type": "module",
  "version": "1.0.0"
}
